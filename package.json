{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build-css": "node-sass --include-path scss scss/app.scss   css/app.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\""
