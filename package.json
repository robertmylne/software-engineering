  "bugs": {
    "url": "https://github.com/electron/electron-quick-start/issues"
  },
  "homepage": "https://github.com/electron/electron-quick-start#readme",
  "devDependencies": {
    "electron": "^1.3.4",
    "electron-connect": "^0.6.0",
    "gulp": "^3.9.1",
    "jquery": "^3.1.1",
    "node-sass": "^3.10.1",
    "nodemon": "^1.10.2"
  },
  "dependencies": {
    "materialize-css": "^0.97.7",
    "vue": "^1.0.0"
  }
}
